{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "EUSCClient": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "react-app",
    "react-app/jest"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/prop-types": "warn",
    "no-unsafe-optional-chaining": 0
  }
}
